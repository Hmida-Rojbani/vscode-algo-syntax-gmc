{
    "name": "algo-syntx-gmc",
    "publisher": "HmidaRojbani",
    "author": "Hmida Rojbani",
    "icon": "icons/icon128.png",
    "displayName": "algo-gmc",
    "description": "Syntax highlighting for algorithms for GMC training",
    "version": "1.1.14",
    "engines": {
        "vscode": ">=1.34.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Hmida-Rojbani/vscode-algo-syntax-gmc"
    },
    "contributes": {
        "languages": [
            {
                "id": "algo",
                "aliases": [
                    "Algorithm",
                    "algo"
                ],
                "extensions": [
                    "algo"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "algo",
                "scopeName": "source.algo",
                "path": "./syntaxes/algo.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "algo",
                "path": "./snippets.json"
            }
        ]
    }
}
