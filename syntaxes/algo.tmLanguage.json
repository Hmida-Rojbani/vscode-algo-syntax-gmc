{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "algo-Aix",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#expression"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#string"
		},
		{
			"include": "#params"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "comment.line",
					"match": "//.*$"
				},
				{
					"name": "comment.block",
					"begin": "\\/\\*",
					"end": "\\*\\/"
				},
				{
					"name": "keyword.control.algo-aix",
					"match": "\\b(ALGORITHM|DO|ELSE_IF|ELSE|PROCEDURE|FUNCTION|RETURN|BREAK|CONTINUE|FROM|TO|THAN)\\b"
				},
				{
					"name": "support.type",
					"match": "\\b(BEGIN|END|VAR|CONST|IF|WHILE|FOR|END_WHILE|END_IF|END_FOR|UNTIL|REPEAT|SWITCH|END_SWITCH)\\b"
				},
				{
					"name": "variable.language",
					"match": "\\b(xxxxxxxx)\\b"
				},
				{
					"name": "storage.type",
					"match": "\\b(CHAR|STRING|ARRAY_OF|BOOLEAN|INTEGER|FLOAT)\\b"
				},
				{
					"name": "constant.numeric",
					"match": "\\b(\\d)\\b"
				},
				{
					"name": "constant.language",
					"match": "\\b(TRUE|FALSE|NULL)\\b"
				},
				{
					"name": "keyword.operator",
					"match": "\\b(:=|=|<>|<|>|<=|>=)\\b"
				},
				{
					"name": "keyword.control",
					"match": "\\b(OR|AND|NOT)\\b"
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.algo-aix",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.algo-aix",
					"match": "\\\\."
				}
			]
		},
		"string": {
			"name": "constant.character",
			"begin": "'",
			"end": "'",
			"patterns": [
				{
					"name": "constant.character.escape.algo-aix",
					"match": "\\\\."
				}
			]
		},
		"expression": {
			"name": "support.function",
			"match": "Display|Put|Next_Line|Length|ToUpper|ToLower|Succ|Pred|Alloc|Realloc"
		},
		"params": {
			"name": "variable.parameter",
			"match": "\\b([_a-zA-Z]\\w*)\\b"
		}
	},
	"scopeName": "source.algo"
}